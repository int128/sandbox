{
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchPackageNames": ["summerwind/actions-runner-dind"],
      "versionScheme": "regex:^v(?<major>\\d+)\\.(?<minor>[\\d\\.]+)-(?<compatibility>ubuntu-[\\d\\.]+)-(?<patch>\\w+)$"
    },
  ],
  "regexManagers": [
    {
      "fileMatch": ["manifests/.+\\.yaml"],
      "matchStrings": [
        "image: (?<depName>.*?):(?<currentValue>.*?) # renovate\\n",
      ],
      "datasourceTemplate": "docker",
    },
  ],
}
